{
  "name": "expo-photosyncapp",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "dotenv -e .env.local -- expo start --web",
    "mock-server": "node scripts/mock-server.js",
    "dev:mock": "concurrently \"npm run mock-server\" \"npm start\"",
    "dev:backend": "dotenv -e .env.local -- expo start"
  },
  "dependencies": {
    "expo": "~48.0.21",
    "expo-background-fetch": "~11.1.1",
    "expo-camera": "~13.2.1",
    "expo-crypto": "~12.2.1",
    "expo-file-system": "~15.2.2",
    "expo-image-manipulator": "~11.1.1",
    "expo-image-picker": "~14.1.1",
    "expo-network": "~5.2.1",
    "expo-secure-store": "~12.3.1",
    "expo-sqlite": "~11.1.1",
    "expo-status-bar": "~1.4.4",
    "expo-task-manager": "~11.1.1",
    "formik": "^2.4.5",
    "jwt-decode": "^4.0.0",
    "react": "18.2.0",
    "react-native": "0.71.14",
    "rxjs": "^7.8.1",
    "yup": "^1.7.1",
    "expo-splash-screen": "~0.18.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "~18.0.27",
    "@types/uuid": "^9.0.2",
    "@types/yup": "^0.29.14",
    "eslint": "^9.39.1",
    "expo-module-scripts": "^5.0.7",
    "jest": "^29.2.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^4.9.4",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "concurrently": "^8.2.0",
    "dotenv-cli": "^7.2.0",
    "@babel/core": "^7.20.0"
  },
  "private": true
}
